{
  "name": "luquinhasbrito/pot-class",
  "description": "POT (PHP OTServ Toolkit) is a PHP toolkit for scripts that work with OTServ database.",
  "type": "library",
  "license": "1.0",
  "authors": [
    {
      "name": "Web Design Technologies",
      "email": "webdesgintechnologies@outlook.com.br"
    }
  ],
  "require": {
    "php" : "^5.5 || ^7.0"
  },
  "repositories": [
    {
        "type": "package",
        "package": {
            "name": "luquinhasbrito/pot-class",
            "version": "master",
            "source": {
                "url": "git@github.com:luquinhasbrito/pot-class.git",
                "type": "git",
                "reference": "origin/master"
            }
        }
    }
  ],
  "autoload": {
    "psr-4": {
      "luquinhasbrito\\pot-class\\": ["src"]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
